{
	"manifest_version": 2,
	"name": "Slack Unread Notifier",
	"version": "0.1",
	"description": "Slackの未読メッセージを通知するChrome拡張機能",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": [
			"lib/jquery.js",
			"js/main.js"
		]
	},
	"permissions": [
		"storage"
	],
	"options_page": "html/setting.html"
}